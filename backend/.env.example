# Configurações do Servidor
PORT=3001
NODE_ENV=development

# Configurações do Banco de Dados PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nota_fiscal_db
DB_USER=postgres
DB_PASSWORD=sua_senha_aqui

# Configurações do Gemini AI
GEMINI_API_KEY=sua_chave_api_gemini_aqui
# Quando true, desativa totalmente o uso de IA
DISABLE_AI=false
# Preferir IA no fluxo híbrido (alguns serviços ainda têm fallback)
PREFER_AI=true

# RAG (Retrieval-Augmented Generation)
ENABLE_RAG=true
RAG_TOP_K=8
RAG_SIMILARITY_THRESHOLD=0.2

# Supabase (necessário para RAG com embeddings)
SUPABASE_URL=https://seu_projeto.supabase.co
SUPABASE_ANON_KEY=sua_chave_anon_supabase
SUPABASE_SERVICE_ROLE_KEY=sua_chave_service_role_supabase

# Configurações de Segurança
JWT_SECRET=sua_chave_secreta_jwt_muito_segura_aqui
BCRYPT_ROUNDS=12

# Configurações de Upload
MAX_FILE_SIZE=10485760

# Configurações do Frontend
FRONTEND_URL=http://localhost:5173
UPLOAD_PATH=./uploads